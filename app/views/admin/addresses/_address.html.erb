<tr>
  <td><%= address.id %></td>
  <td><%= link_to address.user.name, [:admin, address.user] %></td>
  <td><%= address.street_address%></td>
  <td><%= address.city.name %></td>
  <td><%= address.state.name %></td>
  <td><%= address.orders.count %></td>
  <td><%= address.created_at %></td>
  <td><%= link_to "show", [:admin, address] %></td>
  <td><%= link_to "edit", [:admin, address, action: 'edit'] %></td>
  <td><%= link_to "delete", [:admin, address], method: 'delete' %></td>
</tr>
